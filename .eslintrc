{  "root": true,  "parser": "@typescript-eslint/parser",  "env": {    "browser": true,    "es2020": true,    "node": true  },  "settings": {    "react": {      "version": "detect"    }  },  "plugins": ["@typescript-eslint", "typescript-sort-keys", "sort-keys-fix", "unused-imports"],  "extends": [    "eslint:recommended",    "plugin:jsx-a11y/recommended",    "plugin:@typescript-eslint/recommended",    "plugin:typescript-sort-keys/recommended",    "plugin:import/recommended",    "plugin:import/typescript",    "next/core-web-vitals",    "prettier"  ],  "rules": {    "import/no-cycle": "error",    "react/jsx-props-no-spreading": "warn",    "react/jsx-sort-props": "warn",    "react/no-access-state-in-setstate": "error",    "unused-imports/no-unused-imports": "warn",    "unused-imports/no-unused-vars": "warn"  },  "overrides": [    {      "files": ["*.ts", "*.tsx"],      "parserOptions": {        "project": ["./tsconfig.json"]      },      "rules": {        "curly": "error",        "@typescript-eslint/no-unused-vars": "error",        "@typescript-eslint/array-type": [          "warn",          {            "default": "generic"          }        ],        "react/display-name": "off",        "@typescript-eslint/member-ordering": "warn",        "@typescript-eslint/method-signature-style": "warn",        "@typescript-eslint/sort-type-union-intersection-members": "warn",        "arrow-body-style": [2, "always"],        "@typescript-eslint/no-base-to-string": "error",        "@typescript-eslint/no-unnecessary-boolean-literal-compare": "warn",        "@typescript-eslint/ban-types": "error",        "@typescript-eslint/no-empty-interface": "error",        "@typescript-eslint/switch-exhaustiveness-check": "error",        "jsx-a11y/anchor-is-valid": "off",        "react-hooks/exhaustive-deps": "off",        "@typescript-eslint/no-non-null-assertion": "off",        "react/jsx-props-no-spreading": "off",        "jsx-a11y/no-autofocus": "off"      }    },    {      "files": ["pages/_app.tsx"],      "rules": {        "react/jsx-props-no-spreading": "off"      }    }  ]}